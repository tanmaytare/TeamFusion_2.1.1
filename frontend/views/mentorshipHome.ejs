<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="/startupHome.css">
    <title>Startups</title>
</head>
<body>
    <nav class="navbar bg-body-tertiary">
        <div class="container-fluid">
          <a href="/postpage"><span class="navbar-brand mb-0 h1">UpStart</span></a>
        </div>
      </nav>

    <div class="control">
      <a href="/addmentorprogram">Add Course</a>
    </div>

    <div class="searchBlock">
        <form action="/searchmentor" method="post">
          <div class="searchInput">
            <input name="searchele" type="text">
          </div>
          <button type="submit">Search</button>
        </form>
      </div>

   <div class="blogBlock">
    <div class="blogs">
        <% mentorprograms.forEach((mentorprogram) => { %>
          <div class="blog">
            <div class="nameSection">
              <h3><%= mentorprogram.topic  %></h3>
              <p><%= mentorprogram.name %></p>
              <p><%= mentorprogram.qulification %></p>
              <p><%= mentorprogram.date %></p>
              <p><%= mentorprogram.time %></p>
              <p><%= mentorprogram.mode %></p>
              <p><%= mentorprogram.phone %></p>
            </div>
            <div class="descriptionSection">
                <h6>Discription</h6>
                <%= mentorprogram.discription %>
                <h6 style="margin-top:1rem;">Keypoints</h6>
                <p><%= mentorprogram.keypoints %></p>
            </div>
            <div class="imageBlock"><img src="https://www.intelligenthq.com/wp-content/uploads/2018/07/medical-startups.jpg" alt=""></div>
          </div>
        <% }) %>
    </div>
   </div>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</html>